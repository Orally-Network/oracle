{
  "compilerOptions": {
    "baseUrl": "./src",
    "esModuleInterop": true,
    "ignoreDeprecations": "5.0",
    "paths": {
      "Components/*": [
        "./app_frontend/src/components/*"
      ],
      "Services/*": [
        "./app_frontend/src/services/*"
      ],
      "Constants/*": [
        "./app_frontend/src/constants/*"
      ],
      "Interfaces/*": [
        "./app_frontend/src/interfaces/*"
      ],
      "ApiHooks/*": [
        "./app_frontend/src/apiHooks/*"
      ],
      "Utils/*": [
        "./app_frontend/src/utils/*"
      ],
      "Styles/*": [
        "./app_frontend/src/styles/*"
      ],
      "Assets/*": [
        "./app_frontend/assets/*"
      ],
      "Declarations/*": [
        "./declarations/*"
      ],
      "Shared/*": [
        "./app_frontend/src/shared/*"
      ],
      "Pages/*": [
        "./app_frontend/src/pages/*"
      ],
      "OD/*": [
        "./app_frontend/src/outerDeclarations/*"
      ],
      "Providers/*": [
        "./app_frontend/src/providers/*"
      ],
      "Canisters/*": [
        "./app_frontend/src/canisters/*"
      ],
      "Stores/*": [
        "./app_frontend/src/stores/*"
      ],
      "Public/*": [
        "./public/*"
      ]
    },
    "outDir": "./dist",
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "sourceMap": true,
    "checkJs": false,
    "allowJs": true,
    /* Bundler mode */
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    /* Linting */
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
