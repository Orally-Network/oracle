{
  "canisters": {
    "oracle": {
      "candid": "src/oracle/oracle.did",
      "package": "oracle",
      "type": "rust"
    },
    "oracle_factory": {
      "candid": "src/oracle_factory/oracle_factory.did",
      "package": "oracle_factory",
      "type": "rust"
    },
    "ecdsa_proxy": {
      "candid": "src/ecdsa_proxy/ecdsa_proxy.did",
      "package": "ecdsa_proxy",
      "type": "rust"
    },
    "app_frontend": {
      "dependencies": [
        "oracle"
      ],
      "frontend": {
        "entrypoint": "src/app_frontend/src/index.html"
      },
      "source": [
        "src/app_frontend/assets",
        "dist/app_frontend/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
