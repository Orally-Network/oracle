{
  "name": "app_frontend",
  "version": "0.1.0",
  "description": "Internet Computer starter application",
  "keywords": [
    "Internet Computer",
    "Motoko",
    "JavaScript",
    "Canister"
  ],
  "scripts": {
    "optimize": "ic-cdk-optimizer .wasm -o -opt.wasm",
    "build": "webpack --mode production --env NODE_ENV=production",
    "build-stg": "webpack --mode production --env NODE_ENV=staging",
    "prebuild": "npm run generate",
    "start": "webpack serve --mode development --env NODE_ENV=development",
    "start-stg": "webpack serve --mode development --env NODE_ENV=staging",
    "prestart": "npm run generate",
    "generate": "dfx generate",
    "deploy-ic": "dfx build app_frontend --network ic && dfx canister install app_frontend --network ic -m reinstall",
    "deploy-firebase": "npm run build && firebase deploy --only hosting:app-orally",
    "deploy-staging": "npm run build-stg && firebase deploy --only hosting:staging-app-orally",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint - fix src/**/*.{js,jsx,}",
    "format": "prettier - write src/**/*.{js,jsx,} - config .prettierrc.json",
    "graph-build": "graphclient build"
  },
  "devDependencies": {
    "@dfinity/agent": "^0.19.2",
    "@dfinity/candid": "^0.19.2",
    "@dfinity/principal": "^0.19.2",
    "@graphprotocol/client-cli": "^3.0.0",
    "@types/lodash.debounce": "^4.0.8",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "assert": "2.0.0",
    "buffer": "6.0.3",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "events": "3.3.0",
    "firebase-tools": "^13.0.2",
    "html-webpack-plugin": "5.5.0",
    "http-proxy-middleware": "^2.0.6",
    "prettier": "^3.0.3",
    "process": "0.11.10",
    "stream-browserify": "3.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "util": "0.12.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.8.1"
  },
  "engines": {
    "node": "^12 || ^14 || ^16 || ^18"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
